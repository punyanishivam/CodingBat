public int[] front11(int[] a, int[] b) {
  int[] one = new int[1];
  int[] two = new int[2];
  if (a.length == 0 && b.length == 0) {
    return a;
  }
  if (a.length >= 1 && b.length == 0) {
    one[0] = a[0];
    return one;
  }
  if (a.length >= 1 && b.length >= 1) {
    two[0] = a[0];
    two[1] = b[0];
    return two;
  }
  if (a.length == 0 && b.length >= 1) {
    one[0] = b[0];
    return one;
  }
  return a;
}
