public String delDel(String str) {
  String result = "";
  if (str.startsWith("del", 1)) {
    for (int i = 0; i < str.length(); i++) {
      if ((i >= 1) && (i <= 3)) {
        continue;
      }
      else {
        result += str.charAt(i);
      }
    }
  }
  else {
    result = str;
  }
  return result;
}
